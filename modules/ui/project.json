{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/ui/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project ui --web",
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "./modules/ui/.storybook",
        "noOpen": true
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "options": {
        "outputDir": "./modules/ui/dist",
        "configDir": "./modules/ui/.storybook"
      }
    }
  }
}
